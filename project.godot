;   param=value ; assign values to parameters

config_version=5

[application]

config/name="rise-artifact"
config/name_localized={
"en": "Rise Artifact"
}
run/main_scene="res://ui/main_menu.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

card_registry="*res://cards/card_registry.tscn"

[display]

window/size/viewport_width=3840
window/size/viewport_height=2160
window/size/mode=3
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="CardDefense"

[editor]

export/convert_text_resources_to_binary=false

[filesystem]

import/blender/enabled=false

[rendering]

environment/defaults/default_clear_color=Color(0.180392, 0.180392, 0.180392, 1)
